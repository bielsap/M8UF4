<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>MusicLists</display-name>
	<welcome-file-list>
		<welcome-file>principal.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>Login</servlet-name>
		<jsp-file>/login.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Principal</servlet-name>
		<jsp-file>/principal.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>Principal</servlet-name>
		<url-pattern>/principal</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Admin</servlet-name>
		<jsp-file>/admin.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>Admin</servlet-name>
		<url-pattern>/admin</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Usuarios</servlet-name>
		<jsp-file>/usuarios.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>Usuarios</servlet-name>
		<url-pattern>/usuarios</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Benvinguda</servlet-name>
		<jsp-file>/benvinguda.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>Benvinguda</servlet-name>
		<url-pattern>/benvinguda</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Registrar</servlet-name>
		<jsp-file>/alta.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>Registrar</servlet-name>
		<url-pattern>/registrar</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>AltaCancion</servlet-name>
		<jsp-file>/altacancion.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>AltaCancion</servlet-name>
		<url-pattern>/alta-cancion</url-pattern>
	</servlet-mapping>
</web-app>